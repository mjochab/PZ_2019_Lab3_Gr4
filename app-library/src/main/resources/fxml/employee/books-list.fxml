<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXToolbar?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0"
            prefWidth="1280.0" styleClass="background" stylesheets="@../../css/style.css"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.app.library.controller.employee.ListBooksController">
    <top>
        <JFXToolbar opacity="0.88" prefHeight="64.0" prefWidth="61.0" style="-fx-background-color: #937854;"
                    BorderPane.alignment="CENTER">
            <center>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Lista książek" textAlignment="CENTER"
                      wrappingWidth="252.83984375" BorderPane.alignment="CENTER">
                    <font>
                        <Font size="38.0"/>
                    </font>
                </Text>
            </center>
        </JFXToolbar>
    </top>
    <center>
        <AnchorPane prefHeight="490.0" prefWidth="600.0" stylesheets="@../../css/style.css"
                    BorderPane.alignment="CENTER">
            <children>
                <TextField fx:id="searchBooksTextFIeld" layoutX="80.0" layoutY="147.0" onKeyReleased="#findBooks"
                           prefHeight="46.0" prefWidth="1124.0" promptText="Wyszukaj"/>
                <TableView fx:id="booksTable" editable="true" layoutX="79.0" layoutY="214.0" opacity="0.91"
                           prefHeight="436.0" prefWidth="1126.0" style="-fx-background-color: #937854;">
                    <columns>
                        <TableColumn fx:id="idColumn" minWidth="0.0" prefWidth="114.0" text="ID"/>
                        <TableColumn fx:id="titleColumn" prefWidth="599.0" text="Tytuł"/>
                        <TableColumn fx:id="authorColumn" prefWidth="279.0" text="Autor"/>
                        <TableColumn fx:id="quantityColumn" minWidth="0.0" prefWidth="133.0" text="Ilość"/>
                    </columns>
                </TableView>
                <HBox layoutX="78.0" layoutY="20.0" opacity="0.88" prefHeight="100.0" prefWidth="1125.0">
                    <children>
                        <Button fx:id="home" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                minWidth="-Infinity" mnemonicParsing="false" onAction="#goToHome" prefHeight="100.0"
                                prefWidth="122.0" stylesheets="@../../css/style.css">
                            <graphic>
                                <ImageView fx:id="home1" fitHeight="68.0" fitWidth="72.0" pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../../images/library.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <styleClass>
                                <String fx:value="brown_background"/>
                                <String fx:value="border_menu"/>
                            </styleClass>
                        </Button>
                        <Button fx:id="list_of_books" layoutX="10.0" layoutY="10.0" maxHeight="-Infinity"
                                maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                onAction="#goToListBooks" onMouseClicked="#goToListBooks" prefHeight="100.0"
                                prefWidth="160.0" stylesheets="@../../css/style.css">
                            <styleClass>
                                <String fx:value="border_menu"/>
                                <String fx:value="menu_item_active"/>
                            </styleClass>
                            <font>
                                <Font name="System Bold" size="20.0"/>
                            </font>
                            <graphic>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Lista książek">
                                    <font>
                                        <Font size="20.0"/>
                                    </font>
                                </Text>
                            </graphic>
                        </Button>
                        <Button fx:id="orders1" layoutX="170.0" layoutY="10.0" maxHeight="-Infinity"
                                maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                onAction="#goToRealizedOrders" onMouseClicked="#goToRealizedOrders" prefHeight="100.0"
                                prefWidth="160.0" stylesheets="@../../css/style.css">
                            <styleClass>
                                <String fx:value="brown_background"/>
                                <String fx:value="border_menu"/>
                            </styleClass>
                            <font>
                                <Font name="Candara Bold" size="20.0"/>
                            </font>
                            <graphic>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Wypożyczenia">
                                    <font>
                                        <Font size="20.0"/>
                                    </font>
                                </Text>
                            </graphic>
                        </Button>
                        <Button fx:id="users1" layoutX="330.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                onAction="#goToListUsers" onMouseClicked="#goToListUsers" prefHeight="100.0"
                                prefWidth="202.0" stylesheets="@../../css/style.css" textAlignment="CENTER">
                            <styleClass>
                                <String fx:value="border_menu"/>
                                <String fx:value="brown_background"/>
                            </styleClass>
                            <font>
                                <Font name="Candara Bold" size="20.0"/>
                            </font>
                            <graphic>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Użytkownicy">
                                    <font>
                                        <Font size="20.0"/>
                                    </font>
                                </Text>
                            </graphic>
                        </Button>
                        <Button fx:id="add_books1" layoutX="490.0" layoutY="10.0" maxHeight="-Infinity"
                                maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                onAction="#goToAddBooks" prefHeight="100.0" prefWidth="160.0"
                                stylesheets="@../../css/style.css">
                            <styleClass>
                                <String fx:value="border_menu"/>
                                <String fx:value="brown_background"/>
                            </styleClass>
                            <font>
                                <Font name="Candara Bold" size="20.0"/>
                            </font>
                            <graphic>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Dodaj książkę">
                                    <font>
                                        <Font size="20.0"/>
                                    </font>
                                </Text>
                            </graphic>
                        </Button>
                        <Button fx:id="account" layoutX="650.0" layoutY="10.0" maxHeight="-Infinity"
                                maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                onMouseClicked="#goToEmployeeAccount" prefHeight="100.0" prefWidth="160.0"
                                stylesheets="@../../css/style.css">
                            <styleClass>
                                <String fx:value="brown_background"/>
                                <String fx:value="border_menu"/>
                            </styleClass>
                            <font>
                                <Font name="Candara Bold" size="20.0"/>
                            </font>
                            <graphic>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Konto">
                                    <font>
                                        <Font size="20.0"/>
                                    </font>
                                </Text>
                            </graphic>
                        </Button>
                        <Button fx:id="logout1" layoutX="810.0" layoutY="10.0" maxHeight="-Infinity"
                                maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                onMouseClicked="#goToHome" prefHeight="100.0" prefWidth="160.0"
                                stylesheets="@../../css/style.css">
                            <graphic>
                                <ImageView fitHeight="80.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../images/logout.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <styleClass>
                                <String fx:value="border_menu"/>
                                <String fx:value="brown_background"/>
                            </styleClass>
                        </Button>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
